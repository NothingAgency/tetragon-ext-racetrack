<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="com.powerflasher.fdt.ui.MobileApplicationGroup">
	<stringAttribute key="ADL_DESCRIPTOR_FILE" value="bin/@air_descriptor@"/>
	<intAttribute key="AIR_VIEWER_CONF" value="1"/>
	
	<stringAttribute key="ARGUMENTS" value="
		-static-link-runtime-shared-libraries=true
		-swf-version=@swf_version@
		-target-player=@fpv_version@
		-library-path+='{flexSDK}/frameworks/locale/en_US'
		-default-frame-rate=@air_desktop_framerate@
		-default-background-color=0x@air_desktop_bgcolor@
		-default-size=@air_desktop_width@,@air_desktop_height@
		-default-script-limits=@max_recursiondepth@,@max_executiontime@
		-optimize=true
		-strict=true
		-warn-no-constructor=false
		-show-unused-type-selector-warnings=false
		-keep-as3-metadata+=TypeHint
		-keep-as3-metadata+=Resource
		-keep-as3-metadata+=ResourceIndex
		-keep-as3-metadata+=Embed
		-define=CONFIG::IS_WEB_BUILD,false
		-define=CONFIG::IS_AIR_BUILD,true
		-define=CONFIG::IS_DESKTOP_BUILD,true
		-define=CONFIG::IS_MOBILE_BUILD,false
		-define=CONFIG::IS_ANDROID_BUILD,false
		-define=CONFIG::IS_IOS_BUILD,false
		-define=CONFIG::USE_EMBEDDED_RESOURCES,@resource_useEmbed@
	"/>
	
	<booleanAttribute key="DISABLE_COMPILE_PROCESS" value="false"/>
	<stringAttribute key="LAUNCHER_DEPENDENCIES" value="[Self]"/>
	<intAttribute key="LAUNCH_METHOD" value="0"/>
	<stringAttribute key="MAIN_CLASS" value="@path_src_code@/@entry_class@.as"/>
	<intAttribute key="PACKAGE_METHOD" value="0"/>
	
	<booleanAttribute key="PASS_CP" value="true"/>
	<booleanAttribute key="PASS_EXTERNS" value="true"/>
	<booleanAttribute key="PASS_MAIN_CLASS" value="true"/>
	<booleanAttribute key="PASS_RSLS" value="false"/>
	<booleanAttribute key="PASS_SWCS" value="true"/>
	
	<intAttribute key="PLAYER_HEIGHT" value="@air_desktop_width@"/>
	<intAttribute key="PLAYER_WIDTH" value="@air_desktop_height@"/>
	
	<stringAttribute key="POST_COMPILE_ANT" value=""/>
	<stringAttribute key="PRE_COMPILE_ANT" value=""/>
	
	<stringAttribute key="PROJECT_NAME" value="@project_name@"/>
	<stringAttribute key="TARGET_PLATFORM" value="AirDesktop"/>
	<booleanAttribute key="USE_PROJECT_COMPILER_ARGUMENTS" value="false"/>
	
	<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_PATHS">
		<listEntry value="/@project_name@"/>
	</listAttribute>
	<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_TYPES">
		<listEntry value="4"/>
	</listAttribute>
</launchConfiguration>
